int main(void) {
    int n;
    n = 8; /* Queremos o 8º número de Fibonacci */
    int a;
    a = 0;
    int b;
    b = 1;
    int temp;
    int contador;
    contador = 1;

    while (contador < n) {
        temp = a + b;
        a = b;
        b = temp;
        contador = contador + 1;
    }
    
    /* O 8º número de Fibonacci é 21 */
    print(b);
}
